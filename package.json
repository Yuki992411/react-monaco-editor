{
  "name": "react-monaco-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint-es:prefix": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs",
    "lint:es": "pnpm lint-es:prefix .",
    "lint-es:fix": "pnpm lint-es:prefix --fix .",
    "lint:prettier": "prettier --check --ignore-unknown .",
    "lint-prettier:fix": "prettier --write --ignore-unknown .",
    "lint:tsc": "tsc --incremental false --noEmit",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "monaco-editor": "^0.47.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
    "@types/eslint": "^8.56.5",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-flat-gitignore": "^0.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.2.0",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "volta": {
    "node": "20.11.1"
  }
}
